mixin characterCard(character)
	a.character(data-name=character.id,rel=character.name)
		img.character__img(src=character.imgSrc)
		p.character__name= character.name

.container
	img.sf-logo(src="https://vignette2.wikia.nocookie.net/ssb/images/f/f6/SF_Logo.png/revision/latest?cb=20150614010256")

	h1.title Player Select
	.select-container
		for character in characterArray
				+characterCard(character)
			
audio(autobuffer="autobuffer",autoplay="autoplay" loop="true")
	source(src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/813538/02.%20Player%20Select.mp3",  type="audio/ogg")
	
